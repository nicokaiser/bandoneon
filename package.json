{
  "name": "bandoneon",
  "version": "3.2.0",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue,.ts,.json --ignore-path .gitignore .",
    "prettier": "prettier src --write"
  },
  "dependencies": {
    "@tonaljs/note": "^4.6.5",
    "@tonaljs/scale": "^4.6.5",
    "bootstrap": "^5.1.3",
    "pinia": "^2.0.11",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.0",
    "@types/bootstrap": "^5.1.9",
    "@types/node": "^17.0.19",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.7",
    "typescript": "^4.5.5",
    "vite": "^2.8.2",
    "vue-tsc": "^0.31.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended",
      "@vue/eslint-config-typescript",
      "prettier"
    ],
    "globals": {
      "defineEmits": true,
      "defineExpose": true,
      "defineProps": true
    }
  }
}
