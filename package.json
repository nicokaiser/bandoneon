{
  "name": "bandoneon",
  "version": "3.2.1",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue,.ts,.json --ignore-path .gitignore .",
    "prettier": "prettier src --write"
  },
  "dependencies": {
    "@tonaljs/note": "^4.6.5",
    "@tonaljs/scale": "^4.6.5",
    "bootstrap": "^5.1.3",
    "pinia": "^2.0.13",
    "vue": "^3.2.31",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/bootstrap": "^5.1.9",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.34.5"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended",
      "@vue/eslint-config-typescript",
      "prettier"
    ],
    "globals": {
      "defineEmits": true,
      "defineExpose": true,
      "defineProps": true
    }
  }
}
