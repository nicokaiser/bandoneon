{
  "name": "bandoneon",
  "version": "3.2.5",
  "description": "A little JavaScript application that wants to help learning the Alfred Arnold 142 bandoneon.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokaiser/bandoneon.git"
  },
  "author": "Nico Kaiser",
  "license": "MIT",
  "homepage": "https://bandoneon.app",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore .",
    "prettier": "prettier src --write"
  },
  "dependencies": {
    "@tonaljs/note": "^4.6.5",
    "@tonaljs/scale": "^4.6.5",
    "bootstrap": "^5.2.1",
    "pinia": "^2.0.22",
    "vue": "^3.2.39",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@types/bootstrap": "^5.2.4",
    "@types/node": "^18.7.16",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.13"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "plugin:prettier/recommended",
      "@vue/eslint-config-typescript",
      "prettier"
    ],
    "globals": {
      "defineEmits": true,
      "defineExpose": true,
      "defineProps": true
    }
  }
}
